{
  "name": "figma-codegen",
  "version": "1.0.0",
  "description": "CLI tool to generate React/Next.js or HTML/CSS code from Figma files using Tailwind CSS",
  "main": "dist/src/index.js",
  "bin": {
    "figma-codegen": "./dist/src/index.js"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write .",
    "check": "npm run lint && npm run format:check",
    "fix": "npm run lint:fix && npm run format",
    "build": "tsc",
    "test": "jest",
    "start": "node dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theAstralProgrammer0/figma-codegen.git"
  },
  "keywords": [
    "figma",
    "codegen",
    "react",
    "tailwind"
  ],
  "author": "theAstralProgrammer0",
  "license": "GNU",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/theAstralProgrammer0/figma-codegen/issues"
  },
  "homepage": "https://github.com/theAstralProgrammer0/figma-codegen#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.1",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.1.6",
    "ts-jest": "^29.4.2",
    "typescipt": "^1.0.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,css,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "^1.12.2",
    "commander": "^14.0.1",
    "dotenv": "^17.2.2",
    "fs-extra": "^11.3.2"
  }
}
